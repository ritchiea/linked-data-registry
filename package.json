{
  "name": "data-registry",
  "version": "0.0.0",
  "description": "A registry for data packages",
  "main": "index.js",
  "scripts": {
    "push": "(cd registry && ../node_modules/.bin/couchapp push app.js http://seb:seb@127.0.0.1:5984/registry) && (cd owners && ../node_modules/.bin/couchapp push app.js http://seb:seb@127.0.0.1:5984/_users)",
    "start": "COUCH_HOST=127.0.0.1 COUCH_PORT=5984 COUCH_USER=seb COUCH_PASS=seb NODE_PORT=3000 node proxy/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/standard-analytics/data-registry.git"
  },
  "keywords": [ "datapackage", "registry", "open", "data", "CouchDB" ],
  "author": "Sebastien Ballesteros",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/standard-analytics/data-registry/issues"
  },
  "homepage": "https://github.com/standard-analytics/data-registry",
  "dependencies": {
    "semver": "~2.2.1",
    "couchapp": "~0.10.0",
    "padded-semver": "~2.0.5",
    "tv4": "~1.0.11",
    "nano": "~4.2.1",
    "http-proxy": "~0.10.3",
    "express": "~3.4.6",
    "async": "~0.2.9",
    "cookie": "~0.1.0",
    "request": "~2.29.0",
    "npm-user-validate": "0.0.3"
  }
}
